<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="376.0" prefWidth="419.0" stylesheets="@../../css/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Note.LoginController">
   <children>
      <JFXTextField fx:id="username" labelFloat="true" layoutX="79.0" layoutY="122.0" prefHeight="33.0" prefWidth="259.0" promptText="Email">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="password" labelFloat="true" layoutX="81.0" layoutY="194.0" onAction="#handleLoginButtonAction" prefHeight="34.0" prefWidth="259.0" promptText="Password">
         <font>
            <Font size="17.0" />
         </font>
      </JFXPasswordField>
      <JFXButton layoutX="90.0" layoutY="278.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Login" />
      <JFXButton layoutX="223.0" layoutY="278.0" onAction="#handleCancelButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Cancel" />
      <Label layoutX="136.0" layoutY="14.0" prefHeight="70.0" prefWidth="177.0" text="LOGIN" textFill="#ffff8d">
         <font>
            <Font name="Calibri Light Italic" size="48.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="340.0" text="You don't have an account ?" textFill="#fcf8f8">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="sign_b" layoutX="300.0" layoutY="334.0" mnemonicParsing="false" onAction="#toSignUp" styleClass="btn" text="Subscribe" />
      
   </children>
</AnchorPane>
